/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
      Not(in=a, out=NotA);
      Not(in=b, out=NotB);
      And(a=NotA, b=b, out=NotAandB);
      And(a=a, b=NotB, out=AandNotB);
      Or(a=NotAandB, b=AandNotB, out=out);
}